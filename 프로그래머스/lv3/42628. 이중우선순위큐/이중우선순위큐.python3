import heapq

def solution(operations):
    answer = []
    heap = []
    
    for i in operations:
        a, b = i.split(" ")
        
        if a == "I":
            heapq.heappush(heap, int(b))
        else:
            if len(heap) != 0:
                if b == "1":
                    heap.pop()
                else:
                    heapq.heappop(heap)
        
    if len(heap) == 0:
        heap = [0]
    
    return [max(heap), min(heap)]