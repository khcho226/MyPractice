from collections import deque

def solution(pri, loc):
    cnt = 0
    que = deque(enumerate(pri))

    while que:
        pr = 0
        now = que.popleft()
        
        for i in range(len(que)):
            if now[1] < que[i][1]:
                que.append(now)
                pr = 1
                break
                
        if pr == 0:
            cnt += 1
            if now[0] == loc:
                return cnt

    return cnt