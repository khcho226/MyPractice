def func(x):
    num = 0
    for i in range(1, x + 1):
        if x % i == 0:
            num += 1
    
    if num % 2 == 0:
        return True
    return False

def solution(left, right):
    answer = 0
    for i in range(left, right + 1):
        if func(i):
            answer += i
        else:
            answer -= i
    
    return answer