from collections import Counter

def solution(id_list, report, k):
    answer = [0] * len(id_list)
    report = list(set(report))
    lst = []
    
    for i in report:
        a, b = i.split(" ")
        lst.append([a, b])
        
    cnt = Counter(b for a, b in lst)
    lst2 = []
    
    for a, b in cnt.items():
        if b >= k:
            lst2.append(a)
            
    for i in lst:
        if i[1] in lst2:
            answer[id_list.index(i[0])] += 1
    
    return answer