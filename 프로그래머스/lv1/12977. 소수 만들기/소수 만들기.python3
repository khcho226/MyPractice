from itertools import combinations

def solution(nums):
    answer = 0        
    for i in combinations(nums, 3):
        num = sum(i)
        check = True    
        for j in range(2, int(num ** 0.5) + 1):
            if num % j == 0:
                check = False
                break
        
        if check:
            answer += 1
        
    return answer