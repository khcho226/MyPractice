from itertools import combinations

def solution(nums):
    answer = 0
    arr = []

    for i in combinations(nums, 3):
        arr.append(sum(i))
        
    for i in arr:
        check = True
        for j in range(2, int(i ** 0.5) + 1):
            if i % j == 0:
                check = False
                break
        
        if check:
            answer += 1
        
    return answer