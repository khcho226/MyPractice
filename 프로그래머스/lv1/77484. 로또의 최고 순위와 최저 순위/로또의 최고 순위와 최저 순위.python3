def solution(lottos, win_nums):
    correct, zero = 0, 0
    
    for i in lottos:
        if i in win_nums:
            correct += 1
        if i == 0:
            zero += 1
    
    if correct != 0:
        return [7 - correct - zero, 7 - correct]
    else:
        if zero != 0:
            return [7 - correct - zero, 6 - correct]
        else:
            return [6 - correct - zero, 6 - correct]