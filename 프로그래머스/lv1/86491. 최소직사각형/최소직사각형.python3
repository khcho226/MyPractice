def solution(sizes):
    for i in range(len(sizes)):
        if sizes[i][1] > sizes[i][0]:
            temp = sizes[i][0]
            sizes[i][0] = sizes[i][1]
            sizes[i][1] = temp
            
    max0, max1 = 0, 0
    for i in sizes:
        max0 = max(max0, i[0])
        max1 = max(max1, i[1])
        
    return max0 * max1